{
    "name": "erag-lang-sync-inertia",
    "version": "1.4.1",
    "description": "Unified Vue and React language sync helpers for Laravel Inertia",
    "main": "dist/index.js",
    "module": "dist/index.js",
    "types": "dist/index.d.ts",
    "files": [
        "dist"
    ],
    "exports": {
        ".": "./dist/index.js",
        "./vue": "./dist/vue/index.js",
        "./react": "./dist/react/index.js"
    },
    "scripts": {
        "build": "rollup -c rollup.config.cjs",
        "prepare": "npm run build",
        "lint": "prettier --write \"src/**/*.{ts,tsx,js,json,md}\"",
        "lint:check": "prettier --check \"src/**/*.{ts,tsx,js,json,md}\"",
        "type-check": "tsc --noEmit",
        "release": "release-it"
    },
    "peerDependencies": {
        "@inertiajs/core": "^2.0.0",
        "@inertiajs/react": "^2.0.0",
        "@inertiajs/vue3": "^2.0.0",
        "react": "^18.0.0 || ^19.0.0",
        "vue": "^3.0.0"
    },
    "devDependencies": {
        "@inertiajs/core": "^2.2.19",
        "@inertiajs/react": "^2.2.19",
        "@inertiajs/vue3": "^2.2.19",
        "@rollup/plugin-commonjs": "^24.1.0",
        "@rollup/plugin-node-resolve": "^15.2.1",
        "rollup": "^3.0.0",
        "rollup-plugin-typescript2": "^0.32.1",
        "typescript": "^5.0.0",
        "prettier": "^3.7.3",
        "react": "^19.2.0",
        "@types/react": "^19.2.7",
        "release-it": "^19.0.6"
    }
}